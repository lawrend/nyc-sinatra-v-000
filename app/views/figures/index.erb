<h1>List of Figures</h1>

<% Figure.all.each do |figure|%>
<h3>Titles</h3>
  <%figure.titles.each do |title|%>
    <h4><%=title.name%></h4>
  <%end%>
<h3>Name</h3>
  <p><%=figure.name%></p>
<h3>Landmarks</h3>
  <%figure.landmarks.each do |landmark|%>
    <%=landmark.name%>
  <%end%>
  <p>----------------------</p>
<%end%>
